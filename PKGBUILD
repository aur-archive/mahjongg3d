 pkgname=mahjongg3d
 pkgver=0.96
 pkgrel=5
 pkgdesc="An OpenGL Mahjongg game - bin version"
 arch=('i686')
 license=('unknown')
 url="http://www.reto-schoelly.de/mahjongg3d/" 
 depends=('qt3' 'libstdc++5') 
 source=(http://ftp.de-mirror.org/linuxpackages//Slackware-9.1/X11/mahjongg3d/$pkgname-$pkgver-$arch-1LFM.tgz) 
 sha1sums=('6aafea1136164d9c8f669a59f1534f4a5ddd36f7')


package(){ 
tar xvzf ${srcdir}/${pkgname}-${pkgver}-${arch}-1LFM.tgz -C ${pkgdir}
rm -r ${pkgdir}/install
}
